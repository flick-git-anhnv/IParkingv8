using System;
using System.Collections.Generic;
using System.Text;

namespace Futech.Objects
{
    public class MapDetail
    {
        // Constructor
        public MapDetail()
        {
 
        }

        // MapID property
        private int mapID = 0;
        public int MapID
        {
            get { return mapID; }
            set { mapID = value; }
        }

        // ControllerID
        private int controllerID = 0;
        public int ControllerID
        {
            get { return controllerID; }
            set { controllerID = value; }
        }

        // PosX property
        private int posX = 0;
        public int PosX
        {
            get { return posX; }
            set { posX = value; }
        }

        // PosY property
        private int posY = 0;
        public int PosY
        {
            get { return posY; }
            set { posY = value; }
        }
    }
}
