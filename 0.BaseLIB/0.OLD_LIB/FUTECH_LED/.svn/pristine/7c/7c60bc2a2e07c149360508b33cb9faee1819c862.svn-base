using System;
using System.Collections.Generic;
using System.Text;

namespace Futech.Tools
{

    public class ListItem
    {
        private string lname;
        private string lvalue;

        public ListItem()
        { }

        public ListItem(string _name, string _value)
        {
            lvalue = _value;
            lname = _name;
        }

        public string Name
        {
            set { lname = value;}
            get { return lname; }
        }
        public string Value
        {
            set { lvalue = value; }
            get { return lvalue; }
        }
    }
}
